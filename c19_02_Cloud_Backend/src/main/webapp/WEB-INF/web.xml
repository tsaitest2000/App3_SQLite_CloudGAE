<?xml version="1.0" encoding="utf-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee">

    <servlet>
        <servlet-name>ServletHello</servlet-name>
        <servlet-class>com.example.student.myapplication.backend.ServletHello</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletHello</servlet-name>
        <url-pattern>/hello</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ServletAdd</servlet-name>
        <servlet-class>com.example.student.myapplication.backend.ServletAdd</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletAdd</servlet-name>
        <url-pattern>/add</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ServletQuery</servlet-name>
        <servlet-class>com.example.student.myapplication.backend.ServletQuery</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletQuery</servlet-name>
        <url-pattern>/query</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ServletAddEntity</servlet-name>
        <servlet-class>com.example.student.myapplication.backend.ServletAddEntity</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletAddEntity</servlet-name>
        <url-pattern>/addddd</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>QueryHome</servlet-name>
        <servlet-class>com.example.student.myapplication.backend.QueryHome</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>QueryHome</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>

</web-app>