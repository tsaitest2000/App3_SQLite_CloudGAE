<?xml version="1.0" encoding="utf-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee">

    <servlet>
        <servlet-name>ServletAdd</servlet-name>
        <servlet-class>home.backend.ServletAdd</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletAdd</servlet-name>
        <url-pattern>/add</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ServletDelete</servlet-name>
        <servlet-class>home.backend.ServletDelete</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletDelete</servlet-name>
        <url-pattern>/delete</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ServletUpdate</servlet-name>
        <servlet-class>home.backend.ServletUpdate</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletUpdate</servlet-name>
        <url-pattern>/update</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ServletQuery</servlet-name>
        <servlet-class>home.backend.ServletQuery</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletQuery</servlet-name>
        <url-pattern>/query</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>

</web-app>